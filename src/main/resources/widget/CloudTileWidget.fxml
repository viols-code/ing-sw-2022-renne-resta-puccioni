<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<fx:root styleClass="background" stylesheets="@css/styleWidgets.css" type="StackPane"
         xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
         fx:controller="it.polimi.ingsw.view.implementation.gui.widgets.CloudTileWidget">
    <AnchorPane fx:id="pane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                prefHeight="720.0" prefWidth="1080.0" stylesheets="@../gui/css/style.css">


        <Label layoutX="427.0" layoutY="113.0" text="Cloud Tiles">
            <font>
                <Font name="System Bold Italic" size="48.0"/>
            </font>
        </Label>


        <Button styleClass="game-button" layoutX="48.0" layoutY="55.0" mnemonicParsing="false"
                onAction="#showSchoolBoard"
                text="GO BACK TO SCHOOL BOARD">
            <font>
                <Font size="11.0"/>
            </font>
        </Button>
    </AnchorPane>
</fx:root>
